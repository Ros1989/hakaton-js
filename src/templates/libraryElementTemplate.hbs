<section class="library-content">
    <ul class="library-gallery">
        {{#each this}}
        <li class="library-gallery__item">
            <img class="library-gallery__image" src="https://image.tmdb.org/t/p/w500/{{backdrop_path}}"
                alt="{{title}} poster">
            <p class="gallery-item__rating">{{vote_average}}</p>
        </li>
        {{/each}}
    </ul>
</section>