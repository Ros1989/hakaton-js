{{#each this}}
<li class="gallery-item">
    <a href="/{{title}}"><img src="https://image.tmdb.org/t/p/w500{{backdrop_path}}" alt="{{original_title}}"
            class="gallery-item__image" width="" height="" data-value="{{id}}" /></a>
    <p class="gallery-item__name">{{title}}({{release_date}})</p>
    <p class="gallery-item__rating">{{vote_average}}</p>
</li>
{{/each}}